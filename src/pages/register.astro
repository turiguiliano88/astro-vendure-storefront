---
import Base from "../layouts/base.astro";
import App from '../components/register'
import { getActiveCustomer } from "../api/server";

const cookie = Astro.request.headers.get('cookie');
if (cookie) {
	const activeCustomer = (await getActiveCustomer(cookie)).activeCustomer;
    if (activeCustomer) return Astro.redirect('/profile');
}
---

<Base>
<App client:load />
</Base>